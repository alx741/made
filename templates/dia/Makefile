# Replace OUTPUTS with your output PNGs
#
# Dependencies:
# 	Dia

INPUTS = $(shell find . -type f -name '*.dia')
OUTPUTS = $(foreach input, $(INPUTS), $(input:.dia=.png))

all: $(OUTPUTS)

%.png: %.dia
	dia -e $@ $^

clean:
	rm -f *.png
