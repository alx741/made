#!/bin/sh


TEMPLATES=(markdown latex)

if [[ $# -ne 1 ]];
then
    echo
    echo "USE: $0 template"
    echo
    exit 1
fi

exists=false
for i in "${TEMPLATES[@]}"
do
    if [[ "$1" == "$i" ]];
    then
        exists=true
    fi
done

if $exists;
then
    echo
    echo "[!] Template $1 does not exists"
    echo
    exit 1
fi
